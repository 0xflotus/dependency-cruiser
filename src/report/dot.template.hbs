digraph "dependency-cruiser output"{
    ordering=out
    rankdir=LR
    splines=true
    overlap=false
    nodesep=0.16
    fontname="Helvetica"
    fontsize="9"
    compound=true
    node [shape=box style="rounded, filled" fillcolor="#ffffcc" height=0.2 fontname=Helvetica fontsize=9]
    edge [color=black arrowhead=normal fontname="Helvetica" fontsize="9"]

{{#each things}}
    {{#if folder}}subgraph "cluster_{{{folder}}}" {label="{{{folder}}}" style="rounded,filled" fillcolor="#f5f5f5" "{{{source}}}" [label="{{{label}}}"]}
    {{else~}}
    "{{{source}}}"
    {{/if}}
{{/each}}

{{#each things}}
    {{#each dependencies}}
    "{{{../source}}}" -> "{{{resolved}}}" {{#if valid}}{{#if coreModule}}[color="grey"]{{/if}}{{else}}[color="red" penwidth=2.0]{{/if}}
    {{/each}}
{{/each}}
}
